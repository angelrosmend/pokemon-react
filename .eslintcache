             data.push({     id,\n                                  name, \n                                  images, \n                                  description,\n                                  types,\n                                  habitat\n                                /*height: height,\n                                  weight: weight,\n                                  stats: stats,\n                                  habitat: habitat */ })\n                  return data\n                          \n               }\n               let pokemonInfo = gatherPokemonData(idPoke, name.name, images, description, types, habitat/*  sprites, description,types, height, weight,stats, habitat */)\n               setState({\n                data: pokemonInfo,\n                loading: false, \n                error: ''\n               })\n            })\n             }).catch(error => {\n                 setState({\n                     data: [],\n                     loading: false,\n                     error: error\n                 })\n             }); \n            return () => cancel()           \n    }, [urlPoke,language]);\n    return state\n}",{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":2,"column":10,"nodeType":"119","messageId":"120","endLine":2,"endColumn":18},{"ruleId":"117","severity":1,"message":"121","line":1,"column":8,"nodeType":"119","messageId":"120","endLine":1,"endColumn":13},{"ruleId":"117","severity":1,"message":"122","line":2,"column":15,"nodeType":"119","messageId":"120","endLine":2,"endColumn":24},{"ruleId":"117","severity":1,"message":"123","line":22,"column":18,"nodeType":"119","messageId":"120","endLine":22,"endColumn":23},{"ruleId":"117","severity":1,"message":"124","line":22,"column":54,"nodeType":"119","messageId":"120","endLine":22,"endColumn":59},{"ruleId":"113","replacedBy":"125"},{"ruleId":"115","replacedBy":"126"},{"ruleId":"117","severity":1,"mess