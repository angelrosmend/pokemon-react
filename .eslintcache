ue={{data, gotoNextPage, gotoPrevPage, loading, nextPage, prevPage, language, selectEn, selectEs, selectDe}}>\n        {props.children}\n      </PokemonContext.Provider>\n    );\n  };\n\n","/home/angel/Desktop/programming/pruebas/react-pokemon/app/src/helpers/pokemon.js",[],["94","95"],"/home/angel/Desktop/programming/pruebas/react-pokemon/app/src/assets/img/index.js",[],"/home/angel/Desktop/programming/pruebas/react-pokemon/app/src/components/navbar/Navbar.jsx",["96","97","98"],"import React, { useContext, useState } from 'react';\nimport './navbar.css'\nimport {\n  Collapse,\n  Navbar,\n  NavbarToggler,\n  NavbarBrand,\n  Nav,\n  NavItem,\n  NavLink,\n  UncontrolledDropdown,\n  DropdownToggle,\n  DropdownMenu,\n  DropdownItem,\n  NavbarText\n} from 'reactstrap';\nimport { logoPokedex } from '../../assets/img';\nimport { Link } from 'react-router-dom';\nimport { PokemonContext } from '../../context/PokemonContext';\n\nfunction Header(props){\n  const [isOpen, setIsOpen] = useState(false);\n\n  const toggle = () => setIsOpen(!isOpen);\n\n  const {selectEs, selectEn, selectDe} = useContext(PokemonContext)\n\n  return (\n      <Navbar className='shadow' color=\"light\" light expand=\"md\">\n        <NavbarBrand href=\"/\"><img className='logo' src={logoPokedex}/></NavbarBrand>\n        <NavbarToggler onClick={toggle} />\n        <Collapse isOpen={isOpen} navbar>\n          <Nav className=\"ml-auto mr-5\" navbar>\n            <NavItem>\n              <Link to='/favorites' className='nav-link'>Favoritos</Link>\n            </NavItem>\n            <UncontrolledDropdown nav inNavbar>\n              <DropdownToggle nav caret>\n                Lenguaje\n              </DropdownToggle>\n              <DropdownMenu right>\n                <DropdownItem onClick={selectEs}>\n                 Español\n                </DropdownItem>\n                <DropdownItem onClick={selectEn}>\n                  English\n                </DropdownItem>\n                <DropdownItem divider />\n                <DropdownItem onClick={selectDe}>\n                  Deutsch\n                </DropdownItem>\n              </DropdownMenu>\n            </UncontrolledDropdown>\n          </Nav>\n        </Collapse>\n      </Navbar>\n  );\n}\n\nexport default Header;\n","/home/angel/Desktop/programming/pruebas/react-pokemon/app/src/pages/home/Home.jsx",[],"/home/angel/Desktop/programming/pruebas/react-pokemon/app/src/pages/home/pokemonList/PokemonList.jsx",["99","100"],"import React, { useContext } from 'react'\nimport { PokemonContext } from '../../../context/PokemonContext'\nimport './pokemon-home.css'\n\nfunction PokemonList(props) {\n    const {data, loading} = useContext(PokemonContext)\n\n    return (\n        <div className='pokemon-list'>\n            <div className=\"cards-wrapper container-fluid align-items-center\">\n            {props.children}\n            </div>\n        </div>\n    )\n}\n\nexport default PokemonList\n","/home/angel/Desktop/programming/pruebas/react-pokemon/app/src/pages/home/pokemonList/PokemonCard.jsx",["101","102","103"],"import React, { useContext } from 'react'\nimport { Link } from 'react-router-dom'\nimport { PokemonContext } from '../../../context/PokemonContext'\nimport './pokemon-home.css'\n\nfunction PokemonCard({pokemon}) {\n \n  const {id, name, sprites} = pokemon\n    return (\n        <div className='pokemon-card mx-2'>\n           <Link to={`/pokemon/${id}`}>\n            <div className=\"photo\"> \n              <img src={sprites.front_default} alt=\"\"/>\n            </div>\n            </Link>\n           <div className='text-white text-center'> <span>{id} - </span> <span>{name}</span></div>\n        </div>\n    )\n}\n\nexport default PokemonCard\n\n\n{/* <div className=\"img-container\">\n <figure className=\"photo\"> <img className=\"card-img-top\" src={{data.image}}> </figure>\n</div>\n<div className=\"card-footer\">\n<h5 className=\"align-self-center mb-0\">{{data.titulo}}</h5>\n<p className=\"align-self-c